      "post": {
        "description": "登录",
        "tags": [
          "账号"
        ],
        "operationId": "Passport_Login",
        "parameters": [
          {
            "type": "string",
            "x-go-name": "Phone",
            "name": "phone",
            "in": "query"
          },
          {
            "type": "string",
            "x-go-name": "Password",
            "name": "password",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/SUCCESS"
          }
        }
      }
    },
