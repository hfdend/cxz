                "password": {
                  "type": "string",
                  "x-go-name": "Password"
                },
                "phone": {
                  "type": "string",
                  "x-go-name": "Phone"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/SUCCESS"
          }
