      "post": {
        "description": "保存地址",
        "tags": [
          "地址"
        ],
        "operationId": "Address_Save",
        "parameters": [
          {
            "name": "Body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "detail_address": {
                  "description": "详细地址",
                  "type": "string",
                  "x-go-name": "DetailAddress"
                },
                "district_code": {
                  "description": "地区code",
                  "type": "string",
                  "x-go-name": "DistrictCode"
