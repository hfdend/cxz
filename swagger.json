    "/address/del": {
      "post": {
        "description": "删除地址",
        "tags": [
          "地址"
        ],
        "operationId": "Address_Del",
        "parameters": [
          {
            "name": "Body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer",
                  "format": "int64",
                  "x-go-name": "ID"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/responses/SUCCESS"
          }
        }
      }
    },
    "/address/list": {
      "get": {
        "description": "获取地址列表",
        "tags": [
          "地址"
        ],
        "operationId": "Address_List",
